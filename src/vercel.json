{
    "version": 2,
    "builds": [{
        "src": "src/api/index.py",
        "use": "@vercel/python",
        "config": { "maxLamdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/api/index.py"
        }
    ]
}